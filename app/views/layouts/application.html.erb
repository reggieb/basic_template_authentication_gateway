<!DOCTYPE html>
<html>
<head>
  <title>Antechamber</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <p><%= user_signed_in? ? link_to('Logout', destroy_user_session_path, :method => :delete) : link_to('Login', new_user_session_path)  %> </p>


  <%= content_tag('p', notice, class: 'notice') if notice %>
  <%= content_tag('p', alert, class: 'alert') if alert %>

<%= yield %>

</body>
</html>
